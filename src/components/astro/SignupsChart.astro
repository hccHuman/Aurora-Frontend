---
import { onMount } from 'astro/client';
import initSignupsChart from './SignupsChart.client';

onMount(() => {
  const el = document.getElementById('week-signups-chart') as HTMLElement | null;
  if (!el) return;
  initSignupsChart(el);
});
---

<div class="w-full h-36" id="week-signups-chart"></div>
