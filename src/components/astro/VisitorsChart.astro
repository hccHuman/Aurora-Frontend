---
import initVisitorsChart from './VisitorsChart.client';

// Client-only initialization
;(function () {
  if (typeof window === 'undefined') return;
  const el = document.getElementById('visitors-chart') as HTMLElement | null;
  if (!el) return;
  initVisitorsChart(el);
})();
---

<div class="w-full h-64" id="visitors-chart"></div>
